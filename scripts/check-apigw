#!/bin/bash

regions="us-east-1
us-east-2
us-west-1
us-west-2
ap-south-1
ap-northeast-1
ap-northeast-2
ap-northeast-3
ap-southeast-1
ap-southeast-2
ca-central-1
cn-north-1
cn-northwest-1
eu-central-1
eu-west-1
eu-west-2
eu-west-3
sa-east-1"

for region in $regions
do
	echo "$region:"
	aws apigateway get-rest-apis --region $region | jq -r '.items[] | .name+","+ .id' | sed 's/^/\t/'
done

